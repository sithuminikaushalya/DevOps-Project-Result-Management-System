{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "frontend/build" }
      },
      {
        "src": "Backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/Backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/Frontend/$1"
      }
    ]
  }
  